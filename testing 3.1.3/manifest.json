{
  "manifest_version": 3,
  "name": "NoteKeeperTESTING",
  "version": "3.1.3",
  "description": "Notes",
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "permissions": ["activeTab", "storage","notifications"],
  "host_permissions": ["<all_urls>"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ]
}
